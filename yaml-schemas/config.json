{
  "$ref": "#/definitions/ShoutConfig",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "ShoutAction": {
      "additionalProperties": false,
      "properties": {
        "commands": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "cwd": {
          "type": "string"
        },
        "name": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "commands"
      ],
      "type": "object"
    },
    "ShoutConfig": {
      "additionalProperties": false,
      "properties": {
        "quick_actions": {
          "items": {
            "$ref": "#/definitions/ShoutAction"
          },
          "type": "array"
        },
        "servers": {
          "items": {
            "$ref": "#/definitions/ShoutServer"
          },
          "type": "array"
        }
      },
      "required": [
        "servers",
        "quick_actions"
      ],
      "type": "object"
    },
    "ShoutServer": {
      "additionalProperties": false,
      "properties": {
        "connection": {
          "$ref": "#/definitions/ShoutServerConnection"
        },
        "name": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "connection"
      ],
      "type": "object"
    },
    "ShoutServerConnection": {
      "additionalProperties": false,
      "properties": {
        "host": {
          "type": "string"
        },
        "passphrase": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "privateKey": {
          "type": "string"
        },
        "username": {
          "type": "string"
        }
      },
      "required": [
        "host",
        "username"
      ],
      "type": "object"
    }
  }
}
